	<!-- change F117 LGB Turret's min pitch -->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TTurretUnitDescriptor" name="F117 LGB Turret">
		<matchconditions>
			<matchcondition type="referencedby" table="TWeaponManagerModuleDescriptor">
				<matchconditions>
					<matchcondition type="property" property="_ShortDatabaseName">WeaponDescriptor_Unit_F117_Nighthawk</matchcondition>
				</matchconditions>
			</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="AngleRotationMinPitch" type="Float32">-0.78</change>
			<!--  change operation="set" property="ProductionPrice" key="0" type="Int32">5</change>     -->
		</changes>
	</ndfpatch>

	<!-- change B-5 Turret's min pitch -->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TTurretTwoAxisDescriptor" name="B-5 Tail Gun Turret">
		<matchconditions>
			<matchcondition type="referencedby" table="TWeaponManagerModuleDescriptor">
				<matchconditions>
					<matchcondition type="property" property="_ShortDatabaseName">WeaponDescriptor_Unit_H5_NK</matchcondition>
				</matchconditions>
			</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="AngleRotationMinPitch" type="Float32">-1.5</change>
			<!--  change operation="set" property="ProductionPrice" key="0" type="Int32">5</change>     -->
		</changes>
	</ndfpatch>


	<!-- change price of 2800m F117-->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TUniteAuSolDescriptor" name="F117 price">
		<matchconditions>
			<matchcondition type="property" property="NameInMenuToken">79CBBA1822081000</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="ProductionPrice" key="0" type="Int32">200</change>
			<!--  change operation="set" property="ProductionPrice" key="0" type="Int32">5</change>     -->
		</changes>
	</ndfpatch>

	<!-- change property of 1000kg bomb -->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TAmmunition" name="1000kg bomb">
		<matchconditions>
			<matchcondition type="property" property="Caliber">2C1C048100000000</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="RadiusSplashSuppressDamages" type="Float32">63024</change>
			<change operation="set" property="RadiusSplashPhysicalDamages" type="Float32">31512</change>
			<change operation="set" property="PhysicalDamages" type="Float32">38</change>
			<!--  change operation="set" property="ProductionPrice" key="0" type="Int32">5</change>     -->
		</changes>
	</ndfpatch>
