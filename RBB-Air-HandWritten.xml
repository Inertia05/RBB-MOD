	<!-- change property of 1000kg GBU-27/B Paveway III bomb F117-->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TAmmunition" name="F117 1000kg LGB">
		<matchconditions>
			<matchcondition type="property" property="Name">81F0311100000000</matchcondition>
		</matchconditions>
		<changes>
			<change operation="set" property="TempsEntreDeuxSalves" type="Float32">3</change>
			<change property="NbTirParSalves" operation="set" type="UInt32">1</change>
      		<change property="AffichageMunitionParSalve" operation="set" type="UInt32">1</change>
			<change property="SupplyCost" operation="set" type="UInt32">4000</change>
			<!--  change operation="set" property="ProductionPrice" key="0" type="Int32">5</change>     -->
		</changes>
	</ndfpatch>

	<!-- change F117 LGB's salvo count -->
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TWeaponManagerModuleDescriptor" name="F117 Weapon Manager">
		<matchconditions>
			<matchcondition type="property" property="_ShortDatabaseName">WeaponDescriptor_Unit_F117_Nighthawk</matchcondition>
